generator client{
    provider="prisma-client-js"
}
datasource db{
    provider="postgresql"
    url=env("DATABASE_URL")
}
enum Role{
    USER
    VENDOR
    ADMIN
}
model user{
    id String @id @default (uuid())
    fullName String
    email String @unique
    password String
    country String
    role Role
    phone String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}